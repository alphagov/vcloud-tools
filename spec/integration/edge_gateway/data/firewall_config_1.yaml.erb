---
gateway: <%= edge_gateway_name %>
firewall_service:
  policy: drop
  log_default_action: true
  firewall_rules:
  - enabled: true
    description: 'A rule'
    policy: allow
    protocols: 'tcp'
    destination_port_range: '22'
    destination_ip: 10.10.10.20
    source_port_range: 'Any'
    source_ip: 192.0.2.2
